
@{
    Layout = null;
}


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>  إمتحان أونلاين | الصفحة الرئيسية </title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta content="نظام إمتحان أونلاين" name="description" />
    <meta content="MohamedEid" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    <!-- App favicon -->
    <link rel="shortcut icon" href="~/assets/images/logonew.png">
    <!-- App css -->
    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/icons.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/metismenu.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/style.css" rel="stylesheet" type="text/css" />

    <script src="~/assets/js/modernizr.min.js"></script>


</head>


<body>

    <!-- Begin page -->
    <div id="wrapper" class="mt-2">
        <!-- ========== Left Sidebar Start ========== -->
        <div class="left side-menu">
            <div class="p-2 m-0 text-center">
                <h3> <img src="~/assets/images/logoblue.png" width="120" height="50" /></h3>
            </div>

            <!--- Sidemenu -->
            <div id="sidebar-menu" class="pt-0 mt-0">
                <!-- Left Menu Start -->
                <ul class="metismenu text-right" id="side-menu">
                    <li>
                        <a href="@Url.Action("Index","Admin")" class="">
                            <span> الرئيسية </span>   <i class="ti-bookmark"></i>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("AdminMangement","Admin")">
                            <span> إدارة المسئولين </span> <i class="ti-desktop"></i>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("LevlesManagement","Admin")">
                            <span> إدارة المستويات </span> <i class="ti-book"></i>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("DepartmentsManagement","Admin")">
                            <span> إدارة الأقسام </span> <i class="ti-book"></i>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("TeacherMangement","Admin")">
                            <span> إدارة هيئة التدريس </span> <i class="ti-user"></i>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("StudentMangement","Admin")">
                            <span> إدارة الطلاب </span>  <i class="ti-id-badge"></i>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("AcceptedTeacherMangement","Admin")">
                       <span style="font-size:20px;font-family:'Trebuchet MS';"class="text-center text-success"> 
                             @ViewBag.NumberOfTeacherNonAccepted  </span>    
                             <span> طلبات هيئة التدريس </span>  <i class="ti-location-arrow"></i>
                        </a>
                    </li>
                    <li>
                        <a href="javascript: void(0);">
                            <span> الإعدادت </span>  <i class="ti-settings"></i>
                        </a>
                        <ul class="nav-second-level" aria-expanded="false">
                            <li><a href="@Url.Action("DetailsMe","Admin")">تفاصيل الحساب</a></li>
                            <li><a href="@Url.Action("EditMe","Admin")">تعديل الحساب</a></li>
                            <li><a href="@Url.Action("DeleteMe", "Admin")"> حذف الحساب</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="@Url.Action("Logout","Admin")">
                            <span> تسجيل خروج </span>  <i class="ti-export"></i>
                        </a>
                    </li>
                </ul>

            </div>
            <!-- Sidebar -->
            <div class="clearfix"></div>

        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->

        <div class="content-page">
            <!-- Start content -->
            <div class="content">
                <div class="container-fluid">

                    <div class="row">
                        <div class="col-sm-12">
                            <h4 class="header-title m-t-0 m-b-20 text-right">الرئيسية <span class="ti-bookmark"> </span></h4>

                            <h3 class="text-right"> <span>@ViewBag.Name</span>   <span>أهلا بك </span><span class="ti-user"></span></h3>
                            <hr />
                            <p class="text-center w3-hover-text-deep-orange" style="font-size:20px" >
                              إحصائيات 
                            </p>
                            <div class="panel panel-default text-center" style="width:100%;">
                                <div class="panel-heading">
                                    عدد الطلاب المسجلين
                                </div>
                                <div class="panel-body">
                                    <h5> @ViewBag.NumberStudent <i class="ti-id-badge"></i> </h5>
                                </div>

                            </div>

                            <div class="panel panel-default text-center" style="width:100%;">
                                <div class="panel-heading">
                                    عدد المدرسين المقبولين
                                </div>
                                <div class="panel-body">
                                    <h5> @ViewBag.NumberTeacherOk <i class="ti-user"></i> </h5>
                                </div>

                            </div>
                            <div class="panel panel-default text-center" style="width:100%;">
                                <div class="panel-heading">
                                    عدد المدرسين تحت القبول
                                </div>
                                <div class="panel-body">
                                    <h5> @ViewBag.NumberTeacherNon <i class="ti-user"></i> </h5>
                                </div>

                            </div>
                            <div class="panel panel-default text-center" style="width:100%;">
                                <div class="panel-heading">
                                    عدد المسئولين
                                </div>
                                <div class="panel-body">
                                    <h5> @ViewBag.NumberAdmins <i class="ti-user"></i> </h5>
                                </div>
                            </div>
                            <div class="panel panel-default text-center" style="width:100%;">
                                <div class="panel-heading">
                                    عدد الاعلانات التي تمت من خلال المدرسين
                                </div>
                                <div class="panel-body">
                                    <h5> @ViewBag.NumberAnnouncement <i class="ti-announcement"></i> </h5>
                                </div>
                            </div>
                            <div class="panel panel-default text-center" style="width:100%;">
                                <div class="panel-heading">
                                    عدد المواضيع
                                </div>
                                <div class="panel-body">
                                    <h5> @ViewBag.NumbeSubjeects <i class="ti-file"></i> </h5>
                                </div>
                            </div>
                            <div class="panel panel-default text-center" style="width:100%;">
                                <div class="panel-heading">
                                    عدد الامتحانات
                                </div>
                                <div class="panel-body">
                                    <h5> @ViewBag.NumberExams <i class="ti-agenda"></i> </h5>
                                </div>
                            </div>
                            <div class="panel panel-default text-center" style="width:100%;">
                                <div class="panel-heading">
                                    عدد المستويات
                                </div>
                                <div class="panel-body">
                                    <h5> @ViewBag.NumberLevels <i class="ti-book"></i> </h5>
                                </div>
                            </div>
                            <div class="panel panel-default text-center" style="width:100%;">
                                <div class="panel-heading">
                                    عدد الأقسام
                                </div>
                                <div class="panel-body">
                                    <h5> @ViewBag.NumberDepartments <i class="ti-book"></i> </h5>
                                </div>
                            </div>
                            <div class="panel panel-default text-center" style="width:100%;">
                                <div class="panel-heading">
                                    عدد الاسئلة الإختيارية
                                </div>
                                <div class="panel-body">
                                    <h5> @ViewBag.NumberQMCQ <i class="ti-help"></i> </h5>
                                </div>
                            </div>
                            <div class="panel panel-default text-center" style="width:100%;">
                                <div class="panel-heading">
                                    عدد الاسئلة التصحيحة
                                </div>
                                <div class="panel-body">
                                    <h5> @ViewBag.NumberQTAF <i class="ti-help"></i> </h5>
                                </div>
                            </div>

                            <div class="clearfix"></div>
                        </div>
                    </div>

                </div>
                <!-- container -->
                <div class="footer">
                    <div class="pull-right hide-phone">
                    محمد عيد 2020     
                    </div>
                </div>

            </div> <!-- content -->

        </div>


        <!-- ============================================================== -->
        <!-- End Right content here -->
        <!-- ============================================================== -->


    </div>
    <!-- END wrapper -->
    <!-- jQuery  -->
    <script src="~/assets/js/jquery.min.js"></script>
    <script src="~/assets/js/popper.min.js"></script>
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/metisMenu.min.js"></script>
    <script src="~/assets/js/waves.js"></script>
    <script src="~/assets/js/jquery.slimscroll.js"></script>

    <!-- App js -->
    <script src="~/assets/js/jquery.core.js"></script>
    <script src="~/assets/js/jquery.app.js"></script>

</body>
</html>